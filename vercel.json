{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "BackUp7": true
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_VERSION": "@git-branch"
  },
  "regions": ["iad1"],
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}
